  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Azure_ERMS_DataSource</DataSourceName>
        <CommandText>SELECT 
    e.Id AS [Employee Id],
    e.FullName,
    e.Email,
    e.[Role],
    e.Manager,
    COUNT(t.Id) AS TotalTasks,
    SUM(CASE WHEN t.Status = 'Completed' THEN 1 ELSE 0 END) AS CompletedTasks,
    CASE 
        WHEN COUNT(t.Id) &gt; 0 
            THEN (SUM(CASE WHEN t.Status = 'Completed' THEN 1 ELSE 0 END) * 100.0 / COUNT(t.Id))
        ELSE 0 
    END AS TaskSuccessRate
FROM Employees e
LEFT JOIN TaskItems t ON e.Id = t.EmployeeId
GROUP BY 
    e.Id, 
    e.FullName,
    e.Email,
    e.[Role],
    e.Manager
</CommandText>
